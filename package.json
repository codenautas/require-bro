{
  "name": "require-bro",
  "description": "require for browsers",
  "version": "0.1.0",
  "files": ["lib"],
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/require-bro",
  "license": "MIT",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "main": "lib/require-bro.js",
  "dependencies": {
  },
  "devDependencies": {
    "casperjs": "~1.1.1",
    "express": "~4.13.4",
    "istanbul-middleware": "~0.2.2",
    "lodash": "~4.12.0",
    "phantomjs-prebuilt": "~2.1.7",

    "best-globals": "~0.2.4",
    "extension-serve-static": "~0.1.6"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test": "node server/pdemo-server.js --use-casper --coverage",
    "test-h": "node server/pdemo-server.js --use-casper --coverage --hold",
    "start": "node example/server.js"
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true
  },
  "eslintConfig": {
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    }
  },
  "qa-control": {
    "package-version": "0.2.0",
    "coverage": 0,
    "run-in": "client",
    "stability": "extending",
    "type": "lib"
  }
}
